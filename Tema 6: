print("Tema_6 ej 1:")
def bubbleSort(llista):
    mida = len(llista)
    for i in range(1, mida):
        for j in range(0, mida-1):

            if llista[j] < llista[j+1]:
                llista[j], llista[j+1] = (llista[j+1], llista[j])

    return llista


llista = [9,5,8,2,5,9,1]
print(llista)
llista = bubbleSort(llista)
