print("Ejercicio 7")
def letra(x):
    if x == "a" or x == "e" or x == "i" or x == "o" or x == "u":
        return True
    else:
        return False
...........................................................................................................................................
print("Ejercicio 8")
def div(x,y):
    if y % x == 0:
        return True
    else:
        return False

