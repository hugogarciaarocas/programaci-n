print("Ejercicio 4")
def mayor2(x,y):
    if x >= y:
        return x
    else:
        return y

def mayor3(x,y,z):
    if mayor2(x,y) == x and mayor2(y,z) == y:
        return x
    if mayor2(x,y) == y and mayor2(y,z) == y:
        return y
    else:
        return z

