print("Ejercicio 11") mal
def primo(x):
    for i in range (2,x):
        if x % i == 0:
            return False
        else:
            return True
...........................................................................................................................................
print("Ejercicio 12")mal

def Mayor_primo(x):
    z = False
    while z == False:
        y = 0
        x = x + 1
        for i in range(2, x):
            if x % i == 0:
                y = y + 1
        if y == 0:
            z = True
        else:
            z = False

    return x

