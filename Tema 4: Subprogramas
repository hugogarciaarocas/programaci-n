print("Ejercicio 13")
def mayus(x):
    x = str(x)
    if 96 < ord(x) < 123:
        y = ord(x) - 32
        return chr(y)
    else:
        return x
...........................................................................................................................................
print("Ejercicio 14")
def hipotenusa(x,y):
    z = (x**2 + y**2)**0.5
    return z
...........................................................................................................................................
print("Ejercicio 15")
def mcd(x,y):
    cont = 1
    z = 0
    while cont <= x and cont <= y:
        if x % cont == 0 and y % cont == 0:
           z = cont
        cont = cont + 1
    return z
