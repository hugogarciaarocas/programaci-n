print("Ejercicio 12")
x = int(input("Introduce un número para aproximar pi: "))
y = 0
z = -1
for i in range (1, x + 1):
    z = z + 2
    if i % 2 == 0:
        y = y + (-1/z)
        print("(" + str(-1) + "/" + str(z) + ")*4")
    elif i % 2 !=0:
        y = y + (1/z)
        print(str("(+") + str(+1) + "/" + str(z) + ")*4")
y = y * 4
print("=" + str(y))

